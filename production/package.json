{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.6"
  },
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "both": "concurrently \"npm run dev\" \"cd ../frontend &&  npm start\"",
    "build-production": "cd ../ && rm -rf production/ && mkdir production && cp -a ./backend/. ./production/ && rm -rf production/node_modules/ && rm production/.env && cd frontend && npm run build && cd ../ && mv frontend/build/ ./production"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "pg": "^8.5.1",
    "suncalc": "^1.8.0"
  }
}
